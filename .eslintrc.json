{
    "env": {
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "use": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "class-methods-use-this": "off",
        "guard-for-in": "off",
        "no-restricted-syntax": "warn",
        "no-underscore-dangle": "warn",
        "import/no-unresolved": "off",
        "no-param-reassign": "warn",
        "global-require": "off",
        "no-shadow": "warn",
        "no-prototype-builtins": "off"
    }
}